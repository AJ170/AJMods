    setVelX(victim, 5 * mobDir[0]);
    setVelZ(victim, 5 * mobDir[2]);
}

if (autocrit == true) {
        setPosition(getPlayerEnt(), Player.getX(), Player.getY() + 1.3, Player.getZ())
