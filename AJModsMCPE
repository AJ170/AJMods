var NameY=42;
var HealthX=140;
var HealthY=90;

function procCmd(command) 
{ 
var cmd = command.split(" "); 
if(cmd[0]=="edit")
{
activatedChangingGUI=1;
}
if(cmd[0]=="finish")
{
activatedChangingGUI=2;
}
}




