function leaveGame(){
    var ctx = com.mojang.minecraftpe.MainActivity.currentMainActivity.get();
    ctx.runOnUiThread(new java.lang.Runnable({ run: function(){
        if(starterUI != null){
            starterUI.dismiss();
			starterUI = null;
        }
        if(closeUI != null){
            closeUI.dismiss();
			closeUI = null;
        }
        if(GUICC != null){
            GUICC.dismiss();
			GUICC = null;
        }
        if(activatedGUI==true)
{
GUI.dismiss();
GUI2.dismiss();
GUI3.dismiss();
GUI4.dismiss();
activatedGUI=false;
}
        
  if(droidPE != null){
            droidPE.dismiss();
			droidPE = null;
        }
        if(gunUI != null){
            gunUI.dismiss();
			gunUI = null;
        }
        if(flyUI != null){
            flyUI.dismiss();
			flyUI = null;
        }
		if(controlUI != null){
			controlUI.dismiss();
			controlUI = null;
		}
    }}));
}

